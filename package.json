{
  "name": "~egapro-dev",
  "version": "3.13.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "scripts/dev",
    "pgadmin": "docker-compose up --build --force-recreate --remove-orphans pgadmin",
    "dev:app": "pnpm --filter app run dev",
    "dev:maildev": "docker-compose up --build maildev",
    "docker:prod": "docker-compose -f docker-compose.build.yml up --build",
    "lint": "pnpm -r run lint",
    "fresh-install": "scripts/fresh-install",
    "check-all": "scripts/check-all",
    "setup-python": "scripts/postinstall",
    "test:app": "pnpm --filter app test",
    "test:e2e:app": "CYPRESS_BASE_URL=${SITE_URL:-http://localhost:3000} pnpm --filter app test:e2e",
    "build:app": "pnpm --filter app build",
    "build": "pnpm build:app"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@socialgouv/releaserc": "^1.1.1",
    "concurrently": "^8.2.0",
    "semantic-release": "^19"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.31.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
