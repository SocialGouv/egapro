{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "#représentation_équilibrée",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "pourcentage_femmes_cadres": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "pourcentage_hommes_cadres": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "pourcentage_femmes_membres": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "pourcentage_hommes_membres": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "motif_non_calculabilité_cadres": {
      "type": "string",
      "enum": [
        "aucun_cadre_dirigeant",
        "un_seul_cadre_dirigeant"
      ]
    },
    "motif_non_calculabilité_membres": {
      "type": "string",
      "enum": [
        "aucune_instance_dirigeante"
      ]
    }
  }
}